;; This file contains functions for comparing vectors, that is, lists;; that only contain numbers.(defun vector-less-than-p (v1 v2)  (cond ((null v2) nil)        ((null v1) t)        ((< (list-length v1)            (list-length v2))         t)        ((> (list-length v1)            (list-length v2))         nil)        ((and (first v1)              (null (first v2)))         nil)        ((and (null (first v1))              (first v2))         t)        ((and (first v1)              (first v2)              (< (first v1)                 (first v2)))         t)        ((and (first v1)              (first v2)              (> (first v1)                 (first v2)))         nil)        (t (vector-less-than-p (cdr v1) (cdr v2)))))